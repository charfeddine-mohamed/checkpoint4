thelist=[3,6,7,5,6,8]
listnumber=len(thelist)
listeven1=thelist[0:listnumber:2]
listodd1=thelist[1:listnumber:2]
def listmultiple(content1):
    x=1
    for y in content1 :
        x *=y
    return x
def listsum(l):
    tot=0
    for val in l:
        tot=tot+val
    return tot
print ("multiple is" ,listmultiple(thelist),"sum is",listsum(thelist))
